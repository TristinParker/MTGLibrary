 is."}], "securityStamp": "oval", "setCode": "SLD", "sourceProducts": {"foil": ["74d884f6-7be9-5043-be22-d2470612fc8a"], "nonfoil": ["7790702b-6561-5e11-9383-ba9ec1d86772"]}, "subsets": ["Adventures of the Little Witch"], "subtypes": ["Vehicle"], "supertypes": ["Legendary"], "text": "When Esika's Chariot enters, create two 2/2 green Cat creature tokens.\nWhenever Esika's Chariot attacks, create a token that's a copy of target token you control.\nCrew 4", "toughness": "4", "type": "Legendary Artifact â€” Vehicle", "types": ["Artifact"], "uuid": "3fe85e6e-22c1-52aa-b845-9e7e8271a0f7"}, {"artist": "Heikala", "artistIds": ["02be2873-0625-4300-af67-9e0a8c4fd662"], "availability": ["paper"], "borderColor": "borderless", "colorIdentity": ["G"], "colors": ["G"], "convertedManaCost": 3.0, "count": 1, "edhrecRank": 3600, "edhrecSaltiness": 0.44, "finishes": ["nonfoil", "foil"], "flavorText": "The journey is more profound than the destination, but the destination can be pretty great too.", "foreignData": [], "frameEffects": ["inverted"], "frameVersion": "2015", "hasFoil": true, "hasNonFoil": true, "identifiers": {"cardKingdomFoilId": "313141", "cardKingdomId": "313140", "cardsphereFoilId": "136529", "cardsphereId": "136530", "mcmId": "822332", "mtgjsonV4Id": "e1e611dd-1935-5090-9c7f-a1b91ae50070", "scryfallCardBackId": "0aeebaf5-8c7d-4636-9e82-8c27447861f7", "scryfallId": "5e7c1797-8f7b-47d2-aed1-edb88d21a0ac", "scryfallIllustrationId": "2f086d27-a394-467d-9df0-0468835db52b", "scryfallOracleId": "e21c8fc6-d4ef-42b6-b11e-d9c931da1387", "tcgplayerProductId": "629655"}, "isEtched": false, "isFoil": true, "isFullArt": true, "isReprint": true, "language": "English", "layout": "normal", "legalities": {"commander": "Legal", "duel": "Legal", "legacy": "Legal", "modern": "Legal", "oathbreaker": "Legal", "penny": "Legal", "predh": "Legal", "vintage": "Legal"}, "manaCost": "{2}{G}", "manaValue": 3.0, "name": "Realms Uncharted", "number": "1895", "originalReleaseDate": "2025-04-22", "prin